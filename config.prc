# Comment next line out if your card has trouble opening a window.
framebuffer-srgb true

# Comment this out to see the glorious frames.
#show-frame-rate-meter true

# Play with this if you don't like the defaults
win-size 1599 799
#win-fixed-size true

text-default-font $THIS_PRC_DIR/data/font/Roboto-Regular.ttf
model-path $THIS_PRC_DIR/data/model
model-path $THIS_PRC_DIR/data/music

